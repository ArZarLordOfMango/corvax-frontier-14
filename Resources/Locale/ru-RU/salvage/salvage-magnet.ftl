salvage-magnet-resources =
    { $resource ->
        [OreIron] Железо
        [OreCoal] Уголь
        [OreQuartz] Кварц
        [OreGold] Золото
        [OreSilver] Серебро
        [OrePlasma] Плазма
        [OreUranium] Уран
        [OreArtifactFragment] Фрагменты артефактов
       *[other] { $resource }
    }
salvage-magnet-resources-count =
    { $count ->
        [1] (мало)
        [2] (умеренно)
        [3] (средне)
        [4] (много)
        [5] (очень много)
       *[other] (огромное количество)
    }
